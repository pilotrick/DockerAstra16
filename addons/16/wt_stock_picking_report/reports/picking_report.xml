<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
         <record id="paperformat_stock_picking_report" model="report.paperformat">
            <field name="name">Stock Picking</field>
            <field name="default" eval="True"/>
            <field name="format">A4</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">5</field>
            <field name="margin_bottom">5</field>
            <field name="margin_left">5</field>
            <field name="margin_right">5</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">0</field>
            <field name="dpi">80</field>
        </record>

        <record id="wt_stock_picking" model="ir.actions.report">
            <field name="name">Stock Picking</field>
            <field name="model">stock.picking</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">wt_stock_picking_report.template_stock_picking_report</field>
            <field name="paperformat_id" ref="wt_stock_picking_report.paperformat_stock_picking_report"/>
            <field name="report_file">wt_stock_picking_report.template_stock_picking_report</field>
            <field name="print_report_name">'Stock Picking - %s' % (object.name)</field>
            <field name="binding_model_id" ref="stock.model_stock_picking"/>
            <field name="binding_type">report</field>
        </record>
    </data>
</odoo>