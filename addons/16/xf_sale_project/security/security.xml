<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data> <!--  noupdate="1" -->
        <record model="ir.rule" id="project_product_line_visibility_rule">
            <field name="name">Project/Product: Rule</field>
            <field name="model_id" ref="model_project_product_line"/>
            <field name="domain_force">[
                '|',
                ('project_id.privacy_visibility', '!=', 'followers'),
                ('project_id.message_partner_ids', 'in', [user.partner_id.id]),
                ]
            </field>
            <field name="groups" eval="[(4,ref('base.group_user'))]"/>
        </record>
    </data>
</odoo>