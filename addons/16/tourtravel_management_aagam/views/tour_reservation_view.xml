        <record id="view_booking_information_form" model="ir.ui.view">
            <field name="name">booking.information.form</field>
            <field name="model">booking.information</field>
            <field name="type">form</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="Booking Information">
                    <header>
                        <button name="action_quotation_send_booking_info" type="object" string="Send By Email" class="oe_highlight"/>
                        <button name="action_confirmed" type="object" string="Confirm" class="oe_highlight"
                         attrs="{'invisible': [('state', '==', 'confirmed')]}"/>
                         <field name="state" widget="statusbar" statusbar_visible="draft,confirmed"/>
                    </header>
                    <sheet>
                        <group>
                            <group>
                            <field name="tour_reservation_id" required="1"/>
                            <field name="total_no_of_pax" required="1"/>
                                <field name="booking_date"/>
                                </group>
                            <group>
                            <field name="total_adult"/>
                            <field name="total_children"/>
                            <field name="total_infant"/>
                                </group>
                            <group>
                                <field name="customer_id"/>
                                <field name="email_id"/>
                                <field name="mobile_num"/>
                            </group>
                        </group>
                        <group string="Tour Information">
                                <group>
                                    <field name="tour_id"/>
                                    <field name="tour_type"/>
                                    <field name="book_accomodation_id"/>
                                </group>
                                <group>
                                    <field name="tour_start_date"/>
                                    <field name="tour_season"/>
                                    <field name="payment_policy"/>
                                </group>
                            <group>
                            <field name="remarks"/>
                                </group>
                                </group>
                        <notebook>
                            <page string="Person Information">
                                <field name="person_info_ids">
                                    <tree>
                                        <field name="name"/>
                                        <field name="gender"/>
                                        <field name="person_type"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
            </field>
        </record>

