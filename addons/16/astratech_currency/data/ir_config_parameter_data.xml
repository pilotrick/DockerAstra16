<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">

    <record id="astratech_api_currency_banks" model="ir.config_parameter">
        <field name="key">astratech.api.currency.banks</field>
        <field name="value">https://api.astratech.com.do/api/currency/banks</field>
    </record>

    <record id="astratech_currency" model="ir.config_parameter">
        <field name="key">astratech.api.currency</field>
        <field name="value">https://api.astratech.com.do/api/currency</field>
    </record>
    <record id="astratech_token" model="ir.config_parameter">
        <field name="key">astratech.api.token</field>
        <field name="value">https://api.astratech.com.do/api/auth/get_tokens</field>
    </record>

</odoo>