<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <!-- CONTACT US PAGE 10 -->
    <template id="sh_corpomate_tmpl_contact_us_10" name="contact us">
        <t t-call="website.layout">
            <div id="wrap" class="oe_structure">
                <div class="oe_structure" />


                <section id="sh_corpomate_theme_section_272" class="s_banner parallax s_parallax_no_overflow_hidden" data-name="Contact Snippet 1(Banner)" style="background-image: none;" data-scroll-background-ratio="0">
                    <span class="s_parallax_bg oe_img_bg oe_custom_bg" style="background-image: url('/sh_corpomate_theme/static/src/img/contact-banner.png'); background-position: 50% 0; background-attachment: fixed;" />
                    <div class="o_we_bg_filter bg-black-75" style="" />
                    <div class="container">
                        <div class="sh_banner">
                            <div class="sh_des">
                                <h2 class="o_default_snippet_text">Contact</h2>
                                <p class="o_default_snippet_text">Lorem ipsum dolor sit amet, consectetur adipisicing elit.</p>
                            </div>
                            <div class="sh_bread">
                                <ul>
                                    <li>
                                        <a href="/" class="btn o_default_snippet_text">Home</a>
                                        <span class="fa fa-chevron-right" />
                                    </li>
                                    <li class="o_default_snippet_text">Contact</li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </section>
                <section id="sh_corpomate_theme_section_273" data-name="Contact Snippet 2" style="background-image: none;" class="o_colored_level">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-4">
                                <div class="sh_content">
                                    <div class="sh_icon">
                                        <span class="fa fa-envelope-o" />
                                    </div>
                                    <div class="sh_des">
                                        <h4 class="o_default_snippet_text">Mail Us On</h4>
                                        <a href="#" class="btn o_default_snippet_text">support@softhealer.com</a>
                                        <a href="#" class="btn o_default_snippet_text">info@softhealer.com</a>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="sh_content">
                                    <div class="sh_icon">
                                        <span class="fa fa-map-marker" />
                                    </div>
                                    <div class="sh_des">
                                        <h4 class="o_default_snippet_text">Visit On</h4>
                                        <p class="o_default_snippet_text">905, Corporate Levels, Ayodhya Chowk, 150 ft.Ring Road, Rajkot.</p>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="sh_content">
                                    <div class="sh_icon">
                                        <span class="fa fa-phone" />
                                    </div>
                                    <div class="sh_des">
                                        <h4 class="o_default_snippet_text">Call Us On</h4>
                                        <a href="#" class="btn o_default_snippet_text">0123456789</a>
                                        <a href="#" class="btn o_default_snippet_text">0123456789</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
                <section id="sh_corpomate_theme_section_274" class="sh_animation_part o_colored_level" data-snippet="sh_corpomate_theme_tmpl_274" data-name="Contact Us Snippet 3" style="background-image: none;">
                    <div class="container">
                        <div class="sh_title sh_animation_part">
                            <h4 class="o_default_snippet_text">
                                Send a
                                <span class="o_default_snippet_text">Message</span>
                            </h4>
                        </div>
                        <section class="s_website_form" data-vcss="001" data-snippet="s_website_form">
                            <div class="container">
                                <form id="contactus_form" action="/website/form/" method="post" enctype="multipart/form-data" class="o_mark_required" data-mark="*" data-model_name="mail.mail" data-success-mode="redirect" data-success-page="/contactus-thank-you" data-pre-fill="true">
                                    <div class="s_website_form_rows row s_col_no_bgcolor">
                                        <div class="form-group col-12 s_website_form_field s_website_form_custom s_website_form_required" data-type="char" data-name="Field" data-visibility-condition="" data-visibility-between="">
                                            <div class="row s_col_no_resize s_col_no_bgcolor">
                                                <label class="d-none col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="contact1">
                                                    <span class="s_website_form_label_content" />
                                                </label>
                                                <div class="col-md-6 col-12">
                                                    <input id="contact1" type="text" class="form-control s_website_form_input" name="name" placeholder="Name" required="" />
                                                </div>
                                                <div class="col-md-6 col-12">
                                                    <input id="contact2" type="tel" class="form-control s_website_form_input" placeholder="Phone" name="phone" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group col-12 s_website_form_field s_website_form_required" data-type="email" data-name="Field">
                                            <div class="row s_col_no_resize s_col_no_bgcolor">
                                                <label class="d-none col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="contact1">
                                                    <span class="s_website_form_label_content" />
                                                </label>
                                                <div class="col-md-6 col-12">
                                                    <input id="contact3" type="email" class="form-control s_website_form_input" name="email_from" placeholder="Email" required="" />
                                                </div>
                                                <div class="col-md-6 col-12">
                                                    <input id="contact5" type="text" class="form-control s_website_form_input" name="subject" placeholder="Subject" required="" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group col-12 s_website_form_field s_website_form_custom s_website_form_required" data-type="text" data-name="Field">
                                            <div class="row s_col_no_resize s_col_no_bgcolor">
                                                <label class="d-none col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="contact1">
                                                    <span class="s_website_form_label_content" />
                                                </label>
                                                <div class="col-sm">
                                                    <textarea id="contact6" class="form-control s_website_form_input" name="description" placeholder="Message.." required="" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group s_website_form_field col-12    s_website_form_dnone" data-name="Field">
                                            <div class="row s_col_no_resize s_col_no_bgcolor">
                                                <label class="col-form-label col-sm-auto s_website_form_label " style="width: 200px">
                                                    <span class="s_website_form_label_content" />
                                                </label>
                                                <div class="col-sm">
                                                    <input type="hidden" class="form-control s_website_form_input" name="email_to" value="info@yourcompany.example.com" />
                                                    <div class="s_website_form_field_description small form-text text-muted">
                        Separate email addresses with a comma.
                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group col-12 s_website_form_submit" data-name="Submit Button">
                                            <div class="s_website_form_label" />
                                            <a href="#" role="button" class="btn btn-primary btn-lg s_website_form_send sh_btn_cm_1 o_default_snippet_text">Send Message</a>
                                            <span id="s_website_form_result" />
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </section>
                    </div>
                </section>


            </div>
        </t>
    </template>


</odoo>