    <template id="sh_corpomate_theme_style_top_bar_1" name="Top Bar Style 1" inherit_id="website.layout" active="False">
  
        <xpath expr="//header" position="before">   
 
 		<!-- 
        <xpath expr="//div[@id='wrapwrap']//header" position="before">  
              -->
                 
        <div class="oe_structure">
            <section id="sh_top_bar_1">
				<div class="container">
					<div class="row">
						<div class="sh_tb_left col-lg-7 col-md-12">
							<span><a t-att-href="'tel: %s' %(res_company.phone)"><i class="fa fa-phone-square"></i>
							<t t-esc="res_company.phone"/>
							</a></span>|
							<span><i class="fa fa-clock-o"></i>Mon - Sat: 7:00 - 17:00</span>|
							<span><a t-att-href="'mailto: %s' %(res_company.email)"><i class="fa fa-envelope-o"></i>
							<t t-esc="res_company.email"/>
							</a></span>
						</div>
						<div class="sh_tb_right col-lg-5 col-md-12">
							<div class="sh_social_icon">
							<a t-if="website.social_facebook" t-att-href="website.social_facebook" target="_blank"><i class="fa fa-facebook"></i></a>
							</div>
							<div class="sh_social_icon">
							<a t-if="website.social_twitter" t-att-href="website.social_twitter" target="_blank"><i class="fa fa-twitter"></i></a>
							</div>
							<div class="sh_social_icon">
							<a t-if="website.social_youtube" t-att-href="website.social_youtube" target="_blank"><i class="fa fa-youtube"></i></a>
							</div>
							<div class="sh_social_icon m-r-0">
							<a t-if="website.social_instagram" t-att-href="website.social_instagram" target="_blank"><i class="fa fa-instagram"></i></a>
							</div>
							

			<!-- LANGUAGE SELECTOR -->
			<t t-set="flags" t-value="True"/>
				<t t-call="website.language_selector">
				<t t-set="_div_classes" t-value="(_div_classes or '') + ' dropdown'"/>
            </t>
            

											
						</div>
											
					
						    
						
				
						
						
						
					</div>
				</div>
			</section>  	 
		</div>
	    	<!-- End your code -->
        </xpath>
    </template>



    <template id="sh_corpomate_theme_style_top_bar_2" name="Top Bar Style 2" inherit_id="website.layout" active="False">
        <xpath expr="//header" position="before">   
           
        <div class="oe_structure">
			
			<!--  start your code from here -->
			<section id="sh_top_bar_2">
				<div class="container">
					<div class="row">
						<div class="col-lg-6 col-sm-12 col-12 align-left">
						
							<div class="sh-element element-editor phone">
								<a t-att-href="'tel: %s' %(res_company.phone)">
									<t t-esc="res_company.phone"/>
									
								</a>
							</div>
							
							<div class="sh-element element-editor email">
								<a t-att-href="'mailto: %s' %(res_company.email)">
									<t t-esc="res_company.email"/>
								</a>								
							</div>
							
						</div>
						<div class="col-lg-6 col-sm-12 col-12 align-right">
							<ul class="social">
							<li>
							<a t-if="website.social_facebook" t-att-href="website.social_facebook" target="_blank"><i class="fa fa-facebook"></i></a>
							</li>
							<li><a t-if="website.social_twitter" t-att-href="website.social_twitter" target="_blank"><i class="fa fa-twitter"></i></a></li>

							</ul>
							<div class="sh-element custom-login">
							<a groups="base.group_public" href="/web/login" title="Login">Login</a>
							
							<!-- <t t-if="website.auth_signup_uninvited == 'b2c'">
								<span>or</span>
								<a href="/web/signup?" title="Register">Register</a>
							</t> -->
							</div>
							
			<!-- LANGUAGE SELECTOR -->
			<t t-set="flags" t-value="True"/>
            <t t-call="website.language_selector">
                <t t-set="_div_classes" t-value="(_div_classes or '') + ' dropdown'"/>
            </t>
						    						
						</div>
					</div><!--row -->
				</div><!--container -->
			</section>
	    	</div>
	    	<!-- End your code -->
			
        </xpath>
    </template>




    
