<template id="sh_corpomate_theme_custom_header_20" priority="18" active="False" inherit_id="website.header_call_to_action" name="Custom Header for theme 20">
    <xpath expr="//div" position="replace">
        <div t-attf-class="oe_structure oe_structure_solo #{_div_classes} sh_header_custom_20">
            <section class="s_text_block d-none" data-snippet="s_text_block" data-name="Text">
                <div class="container">
                    <a href="/contactus" class="btn btn-primary btn_cta">Contact Us</a>
                </div>
            </section>
            <section class='header_20 '>
                <span>Get Quote <i class='fa fa-arrow-right '/></span>
                <a href='#'><i class='fa fa-share-alt'/></a>
                <ul class='header_link_share' style='display:none;'>
                    <li t-if='website.social_facebook'><a t-att-href="website.social_facebook" class='facebbok' data-toggle="popover" data-trigger="hover" data-content="Facebook" ><i class='fa fa-facebook'/></a></li>
                    <li t-if='website.social_twitter'><a t-att-href="website.social_twitter" class='twitter' data-toggle="popover" data-trigger="hover" data-content="Twitter"><i class='fa fa-twitter'/></a></li>
                    <li t-if='website.social_instagram'><a t-att-href="website.social_instagram" class='instagram' data-toggle="popover" data-trigger="hover" data-content="Instagram"><i class='fa fa-instagram'/></a></li>
                    <li t-if='website.social_linkedin'><a t-att-href="website.social_linkedin" class='linkedin' data-toggle="popover" data-trigger="hover" data-content="Linkedin"><i class='fa fa-linkedin'/></a></li>
                </ul>
            </section>
        </div>
    </xpath>
</template>





