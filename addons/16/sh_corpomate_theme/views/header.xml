<template id="sh_corpomate_theme_custom_header_21" priority="18" active="False" inherit_id="website.navbar_nav" name="Custom Header for theme 21 header">
    <xpath expr="//ul[@id='top_menu']" position="inside">
        <div>
            <li><a href="/contactus" class="btn btn-primary btn_cta">Contact Us</a></li>
            <h5 class="sh_social_text">We Are Available On :</h5>
            <div class="social_icons">
                <div class="social_icon icon_1"><a href="https://www.facebook.com/softhealertechnologies" target="_blank"><span class="fa fa-facebook"></span></a></div>
                <div class="social_icon icon_2"><a href="https://www.instagram.com/softhealer_technologies/?hl=en" target="_blank"><span class="fa fa-instagram"></span></a></div>
                <div class="social_icon icon_3"><a href="https://twitter.com/SofthealerTech" target="_blank"><span class="fa fa-twitter"></span></a></div>
                <div class="social_icon icon_4"><a href="https://www.linkedin.com/in/softhealer-technologies/" target="_blank"><span class="fa fa-linkedin"></span></a></div>
            </div>
            <h5 class="sh_social_text_1">Contact Us On : </h5>
            <div class="sh_contact_details_1">
                <div><a href="#+91 7984575681"><span class="fa fa-phone"></span>+7984575681</a></div>
            </div>
            <div class="sh_contact_details_1">
                <div><a href="#info@softhealer.com"><span class="fa fa-envelope-o"></span>info@softhealer.com</a></div>
            </div>
        </div>
    </xpath>
    <xpath expr="//ul[@id='top_menu']" position="before">
        <button type="button" class="cls_sh_menu_close_btn" 
            data-toggle="collapse" data-target="#top_menu_collapse"></button>
    </xpath>
    
</template> 


<template id="sh_corpomate_theme_custom_header_21_toggler" priority="18" active="False" inherit_id="website.navbar_toggler" name="Custom Header for theme 21 header toggler">
    <xpath expr="//button" position="replace">
      
         <t t-call="portal.placeholder_user_sign_in">
            <t t-set="_item_class" t-value="'nav-item '" />
            <t t-set="_link_class" t-value="'nav-link font-weight-bold'" />
        </t>
        <t t-call="portal.user_dropdown">
            <t t-set="_user_name" t-value="true" />
            <t t-set="_icon" t-value="false" />

            <t t-set="_item_class" t-value="'nav-item dropdown'" />
            <t t-set="_link_class" t-value="'nav-link font-weight-bold sh-hide-auth'" />
        </t>
       

        <button type="button" class="navbar_icon" t-attf-class="navbar-toggler #{_toggler_class}" data-toggle="collapse" data-target="#top_menu_collapse">
            <span class="o_not_editable">
                <svg xmlns="http://www.w3.org/2000/svg" width="21" height="21" viewBox="0 0 21 21"><g class="right_bar" transform="translate(-2124 -2665)"><g transform="translate(2124 2665)" fill="none" stroke-width="1.5"><circle cx="2.5" cy="2.5" r="2.5" stroke="none"></circle><circle cx="2.5" cy="2.5" r="1.75" fill="none"></circle></g><g transform="translate(2132 2665)" fill="none" stroke-width="1.5"><circle cx="2.5" cy="2.5" r="2.5" stroke="none"></circle><circle cx="2.5" cy="2.5" r="1.75" fill="none"></circle></g><g transform="translate(2140 2665)" fill="none" stroke-width="1.5"><circle cx="2.5" cy="2.5" r="2.5" stroke="none"></circle><circle cx="2.5" cy="2.5" r="1.75" fill="none"></circle></g><g transform="translate(2124 2673)" fill="none" stroke-width="1.5"><circle cx="2.5" cy="2.5" r="2.5" stroke="none"></circle><circle cx="2.5" cy="2.5" r="1.75" fill="none"></circle></g><g transform="translate(2132 2673)" fill="none" stroke-width="1.5"><circle cx="2.5" cy="2.5" r="2.5" stroke="none"></circle><circle cx="2.5" cy="2.5" r="1.75" fill="none"></circle></g><g transform="translate(2140 2673)" fill="none" stroke-width="1.5"><circle cx="2.5" cy="2.5" r="2.5" stroke="none"></circle><circle cx="2.5" cy="2.5" r="1.75" fill="none"></circle></g><g transform="translate(2124 2681)" fill="none" stroke-width="1.5"><circle cx="2.5" cy="2.5" r="2.5" stroke="none"></circle><circle cx="2.5" cy="2.5" r="1.75" fill="none"></circle></g><g transform="translate(2132 2681)" fill="none" stroke-width="1.5"><circle cx="2.5" cy="2.5" r="2.5" stroke="none"></circle><circle cx="2.5" cy="2.5" r="1.75" fill="none"></circle></g><g transform="translate(2140 2681)" fill="none" stroke-width="1.5"><circle cx="2.5" cy="2.5" r="2.5" stroke="none"></circle><circle cx="2.5" cy="2.5" r="1.75" fill="none"></circle></g></g></svg>
            </span>
        </button>
        
    </xpath>
